{
  "version": 2,
  "functions": {
    "api/transcribe.py": {
      "runtime": "python3.11",
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/batch-generate-vtt",
      "dest": "api/transcribe.py"
    }
  ],
  "crons": [
    {
      "path": "/batch-generate-vtt",
      "schedule": "0 11 * * *"
    }
  ]
}
