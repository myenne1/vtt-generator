{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/batch-generate-vtt",
      "dest": "/api/index.py",
      "methods": ["GET", "POST"]
    }
  ],
  "crons": [
    {
      "path": "/batch-generate-vtt",
      "schedule": "40 21 * * *"
    }
  ]
}