{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python@3.1.7",
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/batch-generate-vtt",
      "dest": "api/index.py"
    }
  ],
  "crons": [
    {
      "path": "/batch-generate-vtt",
      "schedule": "0 11 * * *"
    }
  ]
}
